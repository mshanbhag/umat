//Printing date in standard US format.
class DateTime {
  public static void main(String[] args) {
    String weekDay = "Saturday";
    String month = "July";
    int date = 28;
    int year = 2011;
    System.out.print(weekDay);
    System.out.print(", ");
    System.out.print(month);
    System.out.print(date);
    System.out.print(", ");
    System.out.print(year);
    System.out.println(".");
  }
}
